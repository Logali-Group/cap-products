CREATE VIEW localized_de_com_logali_SelProducts3 AS SELECT
  ProductReview_1.Rating,
  Products_0.Name,
  sum(Products_0.Price) AS TotalPrice
FROM (localized_de_com_logali_Products AS Products_0 LEFT JOIN localized_de_com_logali_ProductReview AS ProductReview_1 ON Products_0.Name = ProductReview_1.Name)
GROUP BY ProductReview_1.Rating, Products_0.Name
ORDER BY Rating;

CREATE VIEW localized_fr_com_logali_SelProducts3 AS SELECT
  ProductReview_1.Rating,
  Products_0.Name,
  sum(Products_0.Price) AS TotalPrice
FROM (localized_fr_com_logali_Products AS Products_0 LEFT JOIN localized_fr_com_logali_ProductReview AS ProductReview_1 ON Products_0.Name = ProductReview_1.Name)
GROUP BY ProductReview_1.Rating, Products_0.Name
ORDER BY Rating;

